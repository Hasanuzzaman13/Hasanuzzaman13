#include<stdio.h>
#include <string.h>
#include <stdlib.h>
#include<math.h>
int main(){
char ch;
char str1[30];
//char str2[20];
scanf("%c",&ch);
printf("%c \n",ch);
scanf("%s",str1);
printf("%s \n",str1);
scanf("\n");
//scanf("%[^\n]%*c",str2)
scanf("%[^\n]s",str1);
printf("%s \n",str1);
return 0;
}
